         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00003-000-00125"/>
                  <hi rend="latintype">Herrn
                        D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb50" type="place">
                     <hi rend="latintype">Wien</hi>
