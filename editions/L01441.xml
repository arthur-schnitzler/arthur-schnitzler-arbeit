         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00002-000-00165"/>Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
