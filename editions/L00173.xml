      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00001-000-00081"/>Herrn </addrLine>
               <addrLine> D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs ref="#pmb50" type="place">Wien</rs>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb412" type="place">I. Grillparzerstrasse 7</rs>
