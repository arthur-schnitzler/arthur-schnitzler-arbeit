         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1897_AS_RBH_3_00008"/>
                  <damage agent="other">
                     <supplied>Herrn Dr.</supplied>
                  </damage>
                  <space unit="chars" quantity="1"/>
                  <hi rend="latintype">Richard Beer Hofmann</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb50" type="place">Wien</rs>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb338" type="place">
                     <hi rend="latintype">I. Wollzeile 15</hi>
