      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00002-000-00061"/>
                  <hi rend="latintype">Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs ref="#pmb41240" type="place">Austria</rs>
