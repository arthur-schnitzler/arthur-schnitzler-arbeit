   <text>
      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="HS.NZ85.1.3170_0053-1"/>
                  <rs type="place" ref="#pmb40583">
                     <hi rend="pre-print">
                        <hi rend="capitals">Dessauerstrasse</hi> 19</hi>
