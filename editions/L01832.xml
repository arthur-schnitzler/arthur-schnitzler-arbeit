            </address>
            <address>
               <addrLine>
                  <pb facs="96.34.1_2_0002"/>Herrn</addrLine>
               <addrLine>Robert Adam.</addrLine>
               <addrLine>
                  <rs ref="#pmb62" type="place">
                     <hi rend="spaced-out">Wien XII</hi>
                  </rs>
