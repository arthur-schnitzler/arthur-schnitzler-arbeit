               <addrLine>
                  <hi rend="latintype">poste restante <rs ref="#pmb40290" type="place">Thiem</rs>
                  </hi>
               </addrLine>
            </address>
         </div>
         <div type="writingSession" n="1">
            <opener>
               <p>
                  <pb facs="MS-SCHNITZLER-B-0008-B-00001-000-00165"/>
                  <hi rend="pre-print">Telegram fra</hi>
                  <space unit="chars" quantity="1"/>
                  <rs ref="#pmb30" type="place">Salzburg</rs>
                  <space unit="chars" quantity="1"/>
                  <hi rend="pre-print">No.</hi> 501<hi rend="pre-print">, Ord</hi> 20<hi rend="pre-print">, den</hi>
                  <space unit="chars" quantity="1"/>
                  <date>22<hi rend="pre-print">/</hi>7<space unit="chars" quantity="1"/>
                     <hi rend="pre-print">189</hi>6</date>
                  <space unit="chars" quantity="1"/>
                  <hi rend="pre-print">Kl.</hi> 11,10<hi rend="pre-print">midd.</hi>
               </p>
            </opener>
            <p>Reise heute <rs ref="#pmb30" type="place">Salzburg</rs> ab, über <rs ref="#pmb45258" type="place">München</rs>
               <space unit="chars" quantity="1"/>
               <rs ref="#pmb168" type="place">Berlin</rs> bin <date>25</date>
               <space unit="chars" quantity="1"/>
               <rs ref="#pmb36300" type="place">Kopenhagen</rs> erwarte Nachricht herzlichst</p>
            <closer>
               <signed>Richard</signed>
            </closer>
         </div>
      </body>
