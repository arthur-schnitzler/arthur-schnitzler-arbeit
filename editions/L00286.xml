      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00001-000-00121"/>Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs ref="#pmb168934" type="place">IX. Frankgasse</rs>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb50">Wien</rs>
