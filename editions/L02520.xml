            <address>
               <addrLine>
                  <pb facs="96.34.2_0072"/>Hrn Ober L.<unclear>g</unclear>r. Rath</addrLine>
               <addrLine>Dr. Robert Adam Pollak</addrLine>
               <addrLine>
                  <rs ref="#pmb63" type="place">Wien XIII</rs>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb1239" type="place">Meidlinger Hptstr 58</rs>.</addrLine>
            </address>
