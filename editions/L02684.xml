         <div type="address">
            <address>
               <addrLine>
                  <pb facs="DLA_85.1.5681_00001"/>
                  <hi rend="capitals">Paul Goldmann<space unit="chars" quantity="1"/>
                     <rs type="place" ref="#pmb182">Paris</rs>
                  </hi>
               </addrLine>
               <addrLine>
                  <hi rend="capitals">
                     <rs type="place" ref="#pmb42671">24 rue Feydeau</rs>
