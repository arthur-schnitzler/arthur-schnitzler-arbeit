      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="HS.NZ85.1.3175_0045"/>
                  <rs type="place" ref="#pmb687">Österreich</rs>.</addrLine>
               <addrLine>Herrn</addrLine>
               <addrLine>Dr. Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb428">Welsberg im Pustertal</rs>
