            <address>
               <addrLine>Frau Gerty von Hofmannsthal</addrLine>
               <addrLine>
                  <rs type="place" ref="#54">Wien IV</rs>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#44531">Mozartgasse 4</rs>
