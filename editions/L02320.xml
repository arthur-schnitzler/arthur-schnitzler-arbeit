         <div type="address">
            <address>
               <addrLine>
                  <pb facs="96.34.2_0040"/>A. S. <rs ref="#pmb150" type="place">Wien XVIII, <hi rend="latintype">Sternwartestrasse</hi> 71</rs>
               </addrLine>
            </address>
            <address>
               <addrLine>Herrn Landesgerichtsrath</addrLine>
               <addrLine>
                  <hi rend="latintype">Dr. Robert Adam Pollak</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb62" type="place">
                     <hi rend="latintype">Wien</hi> XII</rs>.</addrLine>
               <addrLine>
                  <rs ref="#pmb1239" type="place">
                     <hi rend="latintype">Meidlinger Hptstr.</hi> 58</rs>
