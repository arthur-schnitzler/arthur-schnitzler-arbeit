      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-B-00005-B-000-00162"/>Herrn <hi rend="latintype">D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb168" type="place">
                     <hi rend="latintype">Berlin</hi>
                  </rs>
               </addrLine>
