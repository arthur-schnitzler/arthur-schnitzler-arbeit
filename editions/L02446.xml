            <address>
               <addrLine>
                  <pb facs="Georg_Brandes_arkiv_231"/>Herrn</addrLine>
               <addrLine>Georg Brandes</addrLine>
               <addrLine>
                  <rs ref="#36300" type="place">Kopenhagen</rs>
               </addrLine>
               <addrLine>
                  <rs ref="#43570" type="place">Daenemark</rs>
