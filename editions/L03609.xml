      <body>
         <div type="writingSession" n="1">
            <p>
               <pb facs=""/>Meinem lieben Felix Salten in alter Herzlichkeit</p>
            <closer>
               <signed>Arthur Schnitzler</signed>
            </closer>
            <postscript>
               <dateline>
                  <rs type="place" ref="#pmb50">Wien</rs>
                  <space unit="chars" quantity="1"/>
                  <date when="1919-12-02">2. 12. 919</date>
               </dateline>
            </postscript>
         </div>
         <div type="writingSession" n="2">
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="center">
               <pb facs=""/>
               <hi rend="pre-print">
                  <hi rend="capitals">
                     <rs type="work" ref="#pmb30819">Die Schwestern</rs>
                  </hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">
                     <rs type="work" ref="#pmb30819">oder</rs>
                  </hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">
                     <rs type="work" ref="#pmb30819">Casanova in Spa</rs>
                  </hi>
               </hi>
            </p>
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">Ein Lustspiel in
                  Versen</hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">Drei Akte in
               einem</hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">von</hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">Arthur
               Schnitzler</hi>
               </hi>
            </p>
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <date>1919</date>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="capitals">
                     <rs type="org" ref="#pmb29639">S. Fischer ⋅ Verlag</rs>
                     <space unit="chars" quantity="1"/>⋅<space unit="chars" quantity="1"/>
                     <rs type="place" ref="#pmb168">Berlin</rs>
                  </hi>
               </hi>
            </p>
         </div>
