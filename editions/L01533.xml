            <address>
               <addrLine>
                  <pb facs="1905_AS_RBH_1_00052"/>
                  <hi rend="pre-print">Dr. Arthur
                            Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="pre-print">
                     <rs type="place" ref="#pmb133">Wien XVIII.
