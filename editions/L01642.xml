         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1906_AS_RBH_1_00022"/>
                  <hi rend="pre-print">Dr. Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="pre-print">
