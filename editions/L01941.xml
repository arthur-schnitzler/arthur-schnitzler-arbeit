         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1910_AS_RBH_1_00020"/>Abſ.:</addrLine>
               <addrLine>
                  <hi rend="latintype">Schnitzler</hi>
                  <rs type="place" ref="#pmb50">Wien</rs>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb133">
