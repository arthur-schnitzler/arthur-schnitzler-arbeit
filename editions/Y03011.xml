         <div xmlns="" type="writingSession" n="1">
            <opener>
               <pb xmlns="http://www.tei-c.org/ns/1.0"
                   facs="Wienbibliothek_Schnitzler_Salten_-0361"/>
               <dateline xmlns="http://www.tei-c.org/ns/1.0">
                  <date>
                  
                        25.1.<date/>
                     <date/>
                     <date/>
                  </date>
                  <date>
                     <date/>
                  </date>
                  <date/>
               </dateline>
            </opener>
            <opener>
               <dateline xmlns="http://www.tei-c.org/ns/1.0">
                  <date>
                  908<date/>
                     <date/>
                     <date/>
                  </date>
