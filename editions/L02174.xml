         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1914_AS_RBH_1_00013"/>Hrn <hi rend="latintype">Dr. Richard</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">Beerhofmann</hi>
               </addrLine>
               <addrLine>aus <rs type="place" ref="#pmb50">Wien</rs>
