      <body>
         <div type="writingSession" n="1">
            <p>
               <pb facs="Widmungsexemplare_Seite_064"/>Meinem lieben Arthur Schnitzler</p>
            <closer>herzlichst<lb/>
               <signed>FSalten</signed>
            </closer>
            <postscript>
               <dateline>
                  <rs type="place" ref="#pmb50">Wien</rs>, <date>22. 9. 21</date>
               </dateline>
            </postscript>
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="underline" n="1">Felix Salten</hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <hi rend="antiqua">
                     <rs type="work" ref="#pmb76525">Schauen und Spielen</rs>
                  </hi>
               </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">Erſter Band</hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <rs type="work" ref="#pmb143527">Ergebniſſe<lb/>Erlebniſſe</rs>
               </hi>
            </p>
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="right">
               <hi rend="pre-print">Bloße Vernunft, die ſich am Kunſtwerk</hi>
            </p>
            <p rend="right">
               <hi rend="pre-print">reibt, begeht allemal Unzucht.</hi>
            </p>
            <p>
               <space dim="vertical" unit="line" quantity="1"/>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <date>1921</date>. </hi>
            </p>
            <p rend="center">
               <hi rend="pre-print">
                  <rs type="place" ref="#pmb50">Wien</rs> * <rs type="org" ref="#pmb140789">WILA</rs> * <rs type="place" ref="#pmb864">Leipzig</rs>
               </hi>
            </p>
         </div>
