            <address>
               <addrLine>
                  <pb facs="1904_AS_RBH_1_00020"/>Richard Beerhofma<c rendition="#gemination-n"/>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb173">Rodaun</rs>
