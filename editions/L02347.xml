         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00003-000-00133"/>
                  <hi rend="latintype">Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs type="place" ref="#pmb50">Wien</rs>
