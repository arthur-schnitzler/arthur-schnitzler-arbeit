            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00089-B-00002-000-00055-V"/>
                  <hi rend="capitals">
                     <rs type="place" ref="#pmb468">Europe</rs> – <rs type="place" ref="#pmb41240">Austria</rs>
                  </hi>
               </addrLine>
               <addrLine>Herrn</addrLine>
               <addrLine>D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb168815">XVIII. Sternwartestrasse 71</rs>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb50">Wien</rs>
