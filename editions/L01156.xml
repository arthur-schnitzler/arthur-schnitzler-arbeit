   <text type="telegram">
      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1901_AS_RBH_2_00033"/>Richard Beer Hoffmann</addrLine>
               <addrLine>
                  <rs ref="#pmb1110" type="place">Villa Arnstein</rs>
               </addrLine>
               <addrLine>
                  <hi rend="stamp">
                     <rs ref="#pmb44278" type="place">
                        <hi rend="capitals">Pörtschach am See</hi>
                     </rs>
                  </hi>
               </addrLine>
            </address>
         </div>
         <div type="writingSession" n="1">
            <p>
               <pb facs="1901_AS_RBH_2_00033"/>Bitte <rs type="person" ref="#pmb11485">Paul</rs>
                    nachtelegrafieren er möge doch auf einen Tag <anchor xml:id="K_L01156_1" type="commentary"/>
               <rs type="place" ref="#pmb946">fahren</rs>
               <note xml:id="K_L01156_1h" type="commentary">Gemeint dürfte <rs type="place" ref="#pmb946">Vahrn</rs> sein.</note> ko<c rendition="#gemination-m"/>en
                    behufs Rücksprache habe gewünschten Höhenort in Petto <rs type="place" ref="#pmb50822">Klobenstein</rs> Herzlichst</p>
            <closer>
               <signed>Arthur</signed>
            </closer>
         </div>
      </body>
   </text>
