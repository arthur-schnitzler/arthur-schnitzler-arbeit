      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-0008-B-00001-000-00091"/>
                  <hi rend="pre-print">A</hi>n Herrn</addrLine>
               <addrLine>D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs ref="#pmb50" type="place">Wien</rs>
               </addrLine>
               <addrLine>
