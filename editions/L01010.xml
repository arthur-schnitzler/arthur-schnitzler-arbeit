               <p rend="center">
                  <hi rend="pre-print">
                     <pb facs="MS-SCHNITZLER-B-0008-B-00001-000-00329"/>
                     <rs type="person" ref="#pmb43183">S. Giorgio</rs> (<rs type="person" ref="#pmb20935">Andrea Mantegna</rs>)</hi>
