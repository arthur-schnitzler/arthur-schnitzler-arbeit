                  <foreign xml:lang="fra">
                     <hi rend="pre-print">M</hi>onsieur le Dr. Arthur Schnitzler </foreign>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb414">Pension Petter</rs>
               </addrLine>
