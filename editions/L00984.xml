         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1899_AS_RBH_3_00019"/>Herrn <hi rend="latintype">Dr. Richard
                     Beer-Hofmann</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb40832" type="place">
                     <hi rend="latintype">St. Michael in Eppan</hi>.</rs>
