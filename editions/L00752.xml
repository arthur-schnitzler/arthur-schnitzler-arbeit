      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1897_AS_RBH_4_00019"/>Herrn <hi rend="latintype">Dr. Rich.
                     Beer-Hofmann</hi>
               </addrLine>
               <addrLine>
                  <rs ref="#pmb50" type="place">Wien</rs>
               </addrLine>
               <addrLine>
