            <address>
               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00043-B-00001-000-00007"/>
                  <hi rend="latintype">D<hi rend="superscript">r</hi> Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs type="place" ref="#pmb50">Wien</rs>
