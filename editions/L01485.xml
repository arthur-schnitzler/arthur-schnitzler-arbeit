            <address>
               <addrLine>
                  <pb facs="Hs-30885,118_001"/>
                  <hi rend="latintype">Herrn Hugo v. Hofmannsthal</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs type="place" ref="#pmb173">Rodaun b (Wien)</rs>
                  </hi>
               </addrLine>
               <addrLine>
