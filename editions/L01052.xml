      <body>
         <div type="address">
            <address>
               <addrLine>
                  <pb facs="1900_AS_RBH_2_00026"/>Herrn <hi rend="latintype">Dr. Richard</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">Beer-Hofmann</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs ref="#pmb34401" type="place">Altaussee</rs>.</hi>
               </addrLine>
               <addrLine>
