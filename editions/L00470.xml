               <addrLine>
                  <pb facs="MS-SCHNITZLER-B-00003-000-00015"/>Herrn <hi rend="latintype">D<hi rend="superscript">r</hi>
                  </hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">Arthur Schnitzler</hi>
               </addrLine>
               <addrLine>
                  <hi rend="latintype">
                     <rs type="place" ref="#pmb50">Wien</rs>
                  </hi>
               </addrLine>
               <addrLine>
