    <!-- maps arche props to their values via xpath expressions; be aware the current TEI document needs to be encoded as '$item' -->
    <acdh:TeiLookUps collection="editions"> 
        <acdh:hasTitle type="literal">$item//tei:titleStmt/tei:title[@level='a']/text()</acdh:hasTitle>
        <acdh:hasCreator type="resource">app:ifkovits($item)</acdh:hasCreator>
        <acdh:hasCoverageStartDate type="date">archeutils:normalize-date(data($item//tei:correspAction[1]/tei:date/@when), true())</acdh:hasCoverageStartDate>
        <acdh:hasCoverageStartDate type="date">archeutils:normalize-date(data($item//tei:correspAction[1]/tei:notBefore/@when), true())</acdh:hasCoverageStartDate>
        <acdh:hasCoverageEndDate type="date">archeutils:normalize-date(data($item//tei:correspAction[1]/tei:date/@when), false())</acdh:hasCoverageEndDate>
        <acdh:hasCoverageEndDate type="date">archeutils:normalize-date(data($item//tei:correspAction[1]/tei:notAfter/@when), false())</acdh:hasCoverageEndDate>
        <acdh:hasCreatedStartDate type="date">archeutils:normalize-date(data($item//tei:change[1]/@when), true())</acdh:hasCreatedStartDate>
        <acdh:hasCreatedEndDate type="date">archeutils:normalize-date(data($item//tei:change[last()]/@when), false())</acdh:hasCreatedEndDate>
         <acdh:hasPid type="literal_no_lang">$item//tei:idno[@type="handle"]/text()</acdh:hasPid> 
        <acdh:hasNonLinkedIdentifier type="literal_no_lang">normalize-space(string-join($item//tei:sourceDesc//tei:msIdentifier//text(), '; '))</acdh:hasNonLinkedIdentifier>
        <acdh:hasCustomXsl type="no_eval">https://tei4arche.acdh-dev.oeaw.ac.at/xsl/schnitzler-briefe2arche.xsl</acdh:hasCustomXsl> 
    </acdh:TeiLookUps>
    
    <acdh:TeiLookUps collection="indices"> 
        <acdh:hasTitle type="literal">$item//tei:titleStmt/tei:title[@level='a']/text()</acdh:hasTitle>
        <acdh:hasPid type="literal_no_lang">$item//tei:idno[@type="handle"]/text()</acdh:hasPid>
        <acdh:isPartOf type="resource">$item/@xml:base</acdh:isPartOf>
<!--        <acdh:hasCustomXsl type="literal_no_lang">concat('https://tei4arche.acdh-dev.oeaw.ac.at/xsl/', substring-before(data($item/@xml:id), '.'), '.xsl')</acdh:hasCustomXsl>-->
    </acdh:TeiLookUps>
    
    <!-- mapping  tei:person to arche:Person -->
    <acdh:PersonLookUps source="indices/listperson.xml">
        <acdh:hasIdentifier type="resource_many">archeutils:get_entity_id($item)</acdh:hasIdentifier>
        <acdh:hasTitle type="literal" lang="und">normalize-space($item/tei:persName[1]/tei:forename/text()||' '||$item/tei:persName[1]/tei:surname/text())</acdh:hasTitle>
        <acdh:hasPid type="literal_no_lang">$item//tei:idno[@type="handle"]/text()</acdh:hasPid>
    </acdh:PersonLookUps>
    
