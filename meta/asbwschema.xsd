<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.tei-c.org/ns/1.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tei="http://www.tei-c.org/ns/1.0"
   xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified"
   targetNamespace="http://www.tei-c.org/ns/1.0" vc:minVersion="1.1">
   <xs:import namespace="http://www.w3.org/XML/1998/namespace"
      schemaLocation="http://www.w3.org/2001/xml.xsd"/>
   <!-- Schema der XML-Dateien für https://schnitzler-briefe.acdh.oeaw.ac.at/ 
   Dieses Schema wird für die Produktion verwendet und enthält geringfügige Abweichungen zu
   den tatsächlich verwendeten Elementen. Beispielsweise fehlt das back-Element und
   sind alle Bezugnahmen auf die PMB nur mit Raute »#«, nicht aber mit »#pmb« vorgesehen.
   Azf die Möglichkeit hin, dass es jemandem hilfreich ist, publiziert.
   -->   
   
   
   <xs:element name="TEI">
      <xs:complexType mixed="true">
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="tei:facsimile" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:teiHeader" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:text" maxOccurs="unbounded" minOccurs="0"/>
         </xs:choice>
         <xs:attribute ref="xml:id" use="required"/>
         <xs:attribute ref="xml:base" use="required"/>
      </xs:complexType>
      <xs:unique name="unique-xmlid">
         <xs:selector xpath=".//*"/>
         <xs:field xpath="@xml:id"/>
      </xs:unique>
   </xs:element>
   <xs:element name="footNote">
      <xs:complexType mixed="true">
         <xs:choice minOccurs="1" maxOccurs="unbounded">
            <xs:element ref="tei:p" maxOccurs="unbounded" minOccurs="0"/>
         </xs:choice>
         <xs:attribute ref="xml:id" use="required"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="facsimile">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="tei:graphic"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="teiHeader">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:fileDesc"/>
            <xs:element ref="tei:profileDesc"/>
            <xs:element ref="tei:encodingDesc"/>
            <xs:element ref="tei:revisionDesc"/>
         </xs:sequence>
         <xs:attribute name="type" type="xs:NCName"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="encodingDesc">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:refsDecl"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="refsDecl">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:ab"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="ab"/>
   <xs:element name="ref">
      <xs:complexType mixed="true">
         <xs:attribute name="type" use="optional">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="schnitzlerDiary"/>
                  <xs:enumeration value="toLetter"/>
                  <xs:enumeration value="withinCorrespondence"/>
                  <xs:enumeration value="withinCollection"/>
                  <xs:enumeration value="belongsToCorrespondence"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="subtype" use="optional">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="previous_letter"/>
                  <xs:enumeration value="next_letter"/>
                  <xs:enumeration value="see"/>
                  <xs:enumeration value="cf"/>
                  <xs:enumeration value="Cf"/>
                  <xs:enumeration value="See"/>
                  <xs:enumeration value="date-only"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="target" use="required" xml:base="xs:string"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="fileDesc">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:titleStmt"/>
            <xs:element ref="tei:editionStmt"/>
            <xs:element ref="tei:publicationStmt"/>
            <xs:element ref="tei:seriesStmt"/>
            <xs:element ref="tei:sourceDesc"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="titleStmt">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="tei:title"/>
            <xs:element maxOccurs="unbounded" ref="tei:author"/>
            <xs:element ref="tei:editor"/>
            <xs:element ref="tei:funder"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="funder">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:name"/>
            <xs:element ref="tei:address"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="editionStmt">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:edition"/>
            <xs:element ref="tei:respStmt"/>
            <xs:element ref="tei:idno" minOccurs="1" maxOccurs="1"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="edition" type="xs:NCName"/>
   <xs:element name="publicationStmt">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:publisher"/>
            <xs:element ref="tei:pubPlace" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:date" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:availability"/>
            <xs:element ref="tei:idno" minOccurs="0" maxOccurs="1"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="pubPlace" type="xs:string"/>
   <xs:element name="licence">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:p" maxOccurs="unbounded" minOccurs="0"/>
         </xs:sequence>
         <xs:attribute name="target" use="required" type="xs:anyURI"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="availability">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:licence"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="seriesStmt">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:p"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="sourceDesc">
      <xs:complexType>
         <xs:choice minOccurs="1" maxOccurs="2">
            <xs:element ref="tei:listWit" maxOccurs="1" minOccurs="0"/>
            <xs:element ref="tei:listBibl" maxOccurs="1" minOccurs="0"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="correspDesc">
      <xs:complexType mixed="true">
         <xs:all minOccurs="1">
            <xs:element ref="tei:correspAction" minOccurs="1" maxOccurs="6"/>
            <xs:element ref="tei:correspContext" minOccurs="0" maxOccurs="unbounded"/>
         </xs:all>
         <xs:assert
            test="not(descendant::tei:supplied) and descendant::tei:correspAction/tei:date/@n and not(descendant::tei:correspAction[not(position() = 1)]/tei:date/@n)"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="correspContext">
      <xs:complexType>
         <xs:choice>
            <xs:element ref="tei:note" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:ref" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:p" maxOccurs="unbounded" minOccurs="0"/>
         </xs:choice>
         <xs:assert
            test="tei:ref[(@type = 'belongsToCorrespondence' and starts-with(@target, '#')) or ((@type = 'withinCorrespondence' or @type = 'withinCollection') and (@subtype = 'previous_letter' or @subtype = 'next_letter'))]"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="correspAction">
      <xs:complexType>
         <xs:all>
            <xs:element ref="tei:persName" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:placeName" minOccurs="0"/>
            <xs:element ref="tei:date" minOccurs="0"/>
         </xs:all>
         <xs:attribute name="type" use="required">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="sent"/>
                  <xs:enumeration value="received"/>
                  <xs:enumeration value="transmitted"/>
                  <xs:enumeration value="redirected"/>
                  <xs:enumeration value="forwarded"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:assert
            test="tei:date/@when or tei:date/@notAfter or tei:date/@notBefore or not(tei:date)"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="addressee">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:persName"/>
         </xs:sequence>
         <xs:attribute name="full" type="xs:NCName"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="context">
      <xs:complexType/>
   </xs:element>
   <xs:element name="dateSender">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:date"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="placeAddressee">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:placeName"/>
         </xs:sequence>
         <xs:attribute name="ref" type="xs:string"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="placeSender">
      <xs:complexType mixed="true">
         <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:placeName"/>
         </xs:sequence>
         <xs:attribute name="ref" type="xs:string"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="sender">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="tei:persName"/>
         </xs:sequence>
         <xs:attribute name="full" type="xs:NCName"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="listWit">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="tei:witness"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="witness">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:msDesc"/>
         </xs:sequence>
         <xs:attribute name="n" use="required" type="xs:integer"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="msDesc">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:msIdentifier"/>
            <xs:element ref="tei:physDesc"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="msIdentifier">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:country"/>
            <xs:element ref="tei:settlement"/>
            <xs:element ref="tei:repository"/>
            <xs:element ref="tei:idno"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="repository" type="xs:string"/>
   <xs:element name="physDesc">
      <xs:complexType>
         <xs:all>
            <xs:element ref="tei:objectDesc" minOccurs="1" maxOccurs="1"/>
            <xs:element ref="tei:handDesc" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="tei:typeDesc" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="tei:additions" minOccurs="0" maxOccurs="1"/>
         </xs:all>
         <xs:assert
            test="(tei:objectDesc) and (tei:handDesc or tei:typeDesc or tei:objectDesc/tei:desc/@type = 'reproduktion')"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="handDesc">
      <xs:complexType>
         <xs:all>
            <xs:element ref="tei:handNote" minOccurs="0" maxOccurs="unbounded"/>
         </xs:all>
         <xs:attribute name="hands" use="optional" type="xs:integer"/>
         <xs:assert
            test="count(*) >= 1 and not(count(tei:handNote) &lt; @hands) and not(tei:handNote[not(@corresp)] and tei:handNote[2])"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="typeDesc">
      <xs:complexType>
         <xs:all>
            <xs:element ref="tei:p" minOccurs="0" maxOccurs="1"/>
         </xs:all>
      </xs:complexType>
   </xs:element>
   <xs:element name="objectDesc">
      <xs:complexType>
         <xs:all>
            <xs:element ref="tei:desc" minOccurs="1" maxOccurs="unbounded"/>
         </xs:all>
         <xs:assert test="tei:desc/@type"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="handNote">
      <xs:complexType mixed="false">
         <xs:all>
            <xs:element ref="tei:p" minOccurs="0" maxOccurs="1"/>
         </xs:all>
         <xs:attribute name="medium" use="required">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="bleistift"/>
                  <xs:enumeration value="roter_buntstift"/>
                  <xs:enumeration value="blauer_buntstift"/>
                  <xs:enumeration value="gruener_buntstift"/>
                  <xs:enumeration value="schwarze_tinte"/>
                  <xs:enumeration value="blaue_tinte"/>
                  <xs:enumeration value="gruene_tinte"/>
                  <xs:enumeration value="rote_tinte"/>
                  <xs:enumeration value="anderes"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="style" use="required">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="deutsche-kurrent"/>
                  <xs:enumeration value="lateinische-kurrent"/>
                  <xs:enumeration value="nicht_anzuwenden"/>
                  <xs:enumeration value="gabelsberger"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="corresp" type="xs:string" use="optional"/>
         <xs:assert
            test="(not(@corresp) or (starts-with(@corresp, '#') and string(number(substring-after(@corresp, '#'))) != 'NaN') or (@corresp = 'schreibkraft'))"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="stamp">
      <xs:complexType>
         <xs:sequence>
            <xs:choice minOccurs="1" maxOccurs="4">
               <xs:element ref="tei:placeName" minOccurs="0" maxOccurs="1"/>
               <xs:element ref="tei:date" minOccurs="0" maxOccurs="1"/>
               <xs:element ref="tei:time" minOccurs="0" maxOccurs="1"/>
               <xs:element ref="tei:addSpan" minOccurs="0" maxOccurs="1"/>
            </xs:choice>
         </xs:sequence>
         <xs:attribute name="n" use="required" type="xs:integer"/>
         <xs:assert
            test="tei:placeName[(starts-with(@ref, '#') and string(number(substring-after(@ref, '#'))) != 'NaN') or (tokenize(@ref, ' #')[2])] or not(child::tei:placeName)"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="addSpan">
      <xs:complexType mixed="true">
         <xs:all>
            <xs:element ref="tei:unclear" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:orgName" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:supplied" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:damage" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:rs" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:pb" maxOccurs="unbounded" minOccurs="0"/>
         </xs:all>
         <xs:assert
            test="(child::*[1]) or (string-length(normalize-space(.)) &gt; 0)"
         />
      </xs:complexType>
   </xs:element>
   <xs:element name="listBibl">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="tei:biblStruct"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="biblStruct">
      <xs:complexType>
         <xs:all minOccurs="0">
            <xs:element ref="tei:analytic" minOccurs="0"/>
            <xs:element ref="tei:monogr" minOccurs="1"/>
            <xs:element ref="tei:series" minOccurs="0"/>
         </xs:all>
         <xs:attribute name="corresp" type="xs:NCName"/>
      </xs:complexType>
   </xs:element>
   <xs:element name="analytic">
      <xs:complexType>
         <xs:all>
            <xs:element minOccurs="0" ref="tei:date"/>
            <xs:element ref="tei:author" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:title" maxOccurs="1"/>
            <xs:element ref="tei:editor" minOccurs="0" maxOccurs="unbounded"/>
         </xs:all>
      </xs:complexType>
   </xs:element>
   <xs:element name="monogr">
      <xs:complexType mixed="true">
         <xs:all minOccurs="0">
            <xs:element ref="tei:author" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:biblScope" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:editor" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="tei:imprint" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="tei:title" minOccurs="0" maxOccurs="1"/>
         </xs:all>
      </xs:complexType>
   </xs:element>
   <xs:element name="series">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:title"/>
            <xs:element ref="tei:biblScope"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="profileDesc">
      <xs:complexType>
         <xs:sequence minOccurs="1" maxOccurs="2">
            <xs:element ref="tei:langUsage"/>
            <xs:element ref="tei:correspDesc"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="langUsage">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="tei:language"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="language">
      <xs:complexType>
         <xs:simpleContent>
            <xs:extension base="xs:NCName">
               <xs:attribute name="ident" use="required" type="xs:NCName"/>
            </xs:extension>
         </xs:simpleContent>
      </xs:complexType>
   </xs:element>
   <xs:element name="revisionDesc">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:change"/>
         </xs:sequence>
         <xs:attribute name="status" use="required">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="proposed"/>
                  <xs:enumeration value="candidate"/>
                  <xs:enumeration value="approved"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xsd:simpleType name="date-or-gyear-or-gyearmonth">
      <xsd:union memberTypes="xs:date xs:gYearMonth xs:gYear"/>
   </xsd:simpleType>
   <xs:element name="change">
      <xs:complexType mixed="true">
         <xs:attribute name="when" type="date-or-gyear-or-gyearmonth"/>
         <xs:attribute name="who" use="required">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="MAM"/>
                  <xs:enumeration value="GHS"/>
                  <xs:enumeration value="GHS MAM"/>
                  <xs:enumeration value="KI MAM"/>
                  <xs:enumeration value="KI"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:element name="desc">
      <xs:complexType mixed="true">
         <xs:all minOccurs="0">
            <xs:element ref="tei:pb" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:anchor" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:damage" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:handShift" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:hi" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:note" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:rs" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:space" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:supplied" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:c" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:del" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:quote" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:stamp" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:date" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:bibl" maxOccurs="unbounded" minOccurs="0"/>
            <xs:element ref="tei:ref" maxOccurs="unbounded" minOccurs="0"/>
         </xs:all>
         <xs:attribute name="type">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="_blaetter"/>
                  <xs:enumeration value="_seiten"/>
                  <xs:enumeration value="karte"/>
                  <xs:enumeration value="bild"/>
                  <xs:enumeration value="kartenbrief"/>
                  <xs:enumeration value="brief"/>
                  <xs:enumeration value="umschlag"/>
                  <xs:enumeration value="telegramm"/>
                  <xs:enumeration value="widmung"/>
                  <xs:enumeration value="reproduktion"/>
                  <xs:enumeration value="anderes"/>
                  <xs:enumeration value="entwurf"/>
                  <xs:enumeration value="fragment"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="subtype">
            <xs:simpleType>
               <xs:restriction base="xs:NCName">
                  <xs:enumeration value="bildpostkarte"/>
                  <xs:enumeration value="postkarte"/>
                  <xs:enumeration value="briefkarte"/>
                  <xs:enumeration value="visitenkarte"/>
                  <xs:enumeration value="fotokopie"/>
                  <xs:enumeration value="fotografie"/>
                  <xs:enumeration value="fotografische_vervielfaeltigung"/>
                  <xs:enumeration value="ms_abschrift"/>
                  <xs:enumeration value="hs_abschrift"/>
                  <xs:enumeration value="durchschlag"/>
                  <xs:enumeration value="widmung_vorsatzblatt"/>
                  <xs:enumeration value="widmung_titelblatt"/>
                  <xs:enumeration value="widmung_vortitel"/>
                  <xs:enumeration value="widmung_schmutztitel"/>
                  <xs:enumeration value="widmung_umschlag"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="n">
            <xs:simpleType>
               <xs:restriction base="xs:integer"/>
            </xs:simpleType>
         </xs:attribute>
         <xs:assert
            test="
               (parent::tei:incident)
               or (parent::tei:objectDesc and (
               (@type = 'karte') or
